<configuration>
    <system.web>
      <compilation debug="true" targetFramework="4.0" />
      <pages>
        <controls>
          <add src="~/UserControl/UserLogin.ascx" tagPrefix="user" tagName="UserLogin"/>
          <add src="~/UserControl/SpecialBookList.ascx" tagPrefix="user" tagName="SpecialBookList"/>
          <add assembly="BookShopControls" namespace="BookShopControls" tagPrefix="user"/>
        </controls>
      </pages>
    </system.web>
    <connectionStrings>
      <add name="BookShop" connectionString="Data Source=(local);Initial Catalog=BookShop;Integrated Security=True"/>
    </connectionStrings>
</configuration>
