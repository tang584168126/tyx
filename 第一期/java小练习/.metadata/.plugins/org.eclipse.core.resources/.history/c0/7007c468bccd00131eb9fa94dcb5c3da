import java.util.Scanner;
public class Text4 {
	public static void main(String[] args){
		int a,b,c;
		Scanner sc = new Scanner(System.in);
		System.out.print("请输入三个整数：");
		a = sc.nextInt();
		b = sc.nextInt();
		c = sc.nextInt();
		if(a < b){
			if(a < c){
				if(b < c){
					System.out.println(a + "<" + b + "<" + c);
				}else{
					System.out.println(a + "<" + c + "<" + b);
				}
			}else{
				System.out.println(c + "<" + a + "<" + b);
			}
		}else{
			System.out.println("");
		}
	}
}
