package ฒน;

public class Test2 {
	public static void main(String[] args){
		int i = 0 ;
		int j = 0;
		for(i = 1; i <= 8 ; i++){
			if(i % 2 == 0){
				continue;
			}
				
			
			for(j = 1; j <= 8 ; j++){
				System.out.print(i + "+" + j); 
				
				
				
			}
			System.out.print("\n");
			
		}
	}

}
