		int[] sums = new int[10];
		int i = 0;
		Scanner sc = new Scanner(System.in);
		System.out.println("请输入十个数：");
		for(i = 0 ;i < sums.length ; i++){
			sums[i] = sc.nextInt();
			
			
		}
	}

