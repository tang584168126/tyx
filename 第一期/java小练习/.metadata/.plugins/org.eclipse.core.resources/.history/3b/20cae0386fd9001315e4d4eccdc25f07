
public class Test_1 {
	public static void main(String[] args){
		int i = 0, j = 0, k = 0;
		for(i = 0;i <= 10; i++)
		{
			for(j = 0;j<= 20; j++)
			{
				for(k = 10; k<20; k++)
				{
					if(i+j+k==30&&3*i+2*j+k==50)
					{
						System.out.println(i);
						System.out.println(j);
						System.out.println(k);
					}
				}
			}
			
		}
	}

}
