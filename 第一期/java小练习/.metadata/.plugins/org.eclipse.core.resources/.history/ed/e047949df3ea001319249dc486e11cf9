package class14;
/*
 * 数组帮助类
 * */
public class shuzubangzhulei {
	//遍历数组
	public void Show(String []a){
		for (int i = 0; i < a.length; i++) {
			if(a[i]!=null){
				System.out.println(a[i]);
			}
		}
	}
	//删除某一个下标
	public boolean Del(String a[],String name){
		for (int i = 0; i < a.length; i++) {
			if(a[i]!=null){
				
				if(a[i].equals(name)){
					a[i]=null;
				}
			}
		}
		
		return true;
	}
}
